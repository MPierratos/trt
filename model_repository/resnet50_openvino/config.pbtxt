name: "resnet50_openvino"
backend: "openvino"
max_batch_size : 1
input [
  {
    name: "x"
    data_type: TYPE_FP32
    # Triton adds batch dimension automatically
    # Client sends shape [3, 224, 224] per sample
    dims: [ 3, 224, 224 ]
  }
]
output [
  {
    name: "x.45"
    data_type: TYPE_FP32
    # Output shape [1000] per sample
    dims: [ 1000 ]
  }
]

instance_group [{ 
  kind: KIND_CPU
  count: 2
}]

# No dynamic batching - immediate processing for real-time video streams
# Each frame is processed as it arrives (30fps = 33ms per frame)